{
    "plugins": [],
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(x)?$",
        "excludePattern": "(node_modules|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "template": "./node_modules/ojeda-jsdoc-template"
    }
}